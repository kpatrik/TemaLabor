@model AlberletKereso.Models.Alberlet

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{ 
    ViewBag.Title = "Kép hozzáadása";
}

<h1>
    @ViewBag.Title
</h1>

@using (Html.BeginForm("Új kép hozzáadása", "SajatAlberletek", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" />
    <input type="submit" name="Submit" id="Submit" value="Kép feltöltése" />
}